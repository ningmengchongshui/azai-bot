{
  "name": "a-yunzai",
  "version": "3.0.0",
  "description": "A崽机器人https://gitee.com/ningmengchongshui/a-yunzai",
  "author": "ningmengchongshui",
  "license": "GPL-2.0",
  "main": "alemon.config.js",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "afloat dev",
    "app": "ts-node alemon.config.ts",
    "start": "pm2 startOrRestart pm2.config.cjs",
    "stop": "pm2 stop pm2.config.cjs",
    "restart": "pm2 restart pm2.config.cjs",
    "delete": "pm2 delete pm2.config.cjs",
    "logs": "pm2 logs",
    "list": "pm2 list",
    "startup": "pm2 startup",
    "update": "pm2 update",
    "monit": "pm2 monit",
    "save": "pm2 save",
    "kill": "pm2 kill",
    "pull:l": "find . -type d -name '.git' -execdir git pull --all \\;",
    "pull:w": "cd plugins && for /d %i in (*) do (echo Updating %i && cd %i && git pull && cd ..) ",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.ts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "art-template": "^4.13.2",
    "chalk": "^5.3.0",
    "chokidar": "^3.5.3",
    "icqq": "^0.6.1",
    "inquirer": "^9.2.11",
    "ioredis": "^5.3.2",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "moment": "^2.29.4",
    "mysql2": "^3.5.2",
    "node-fetch": "^3.3.2",
    "node-schedule": "^2.1.1",
    "oicq": "^2.3.1",
    "redis": "^4.6.10",
    "sequelize": "^6.32.1",
    "sqlite3": "^5.1.6",
    "yaml": "^2.3.3"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.200",
    "@types/node": "^20.8.5",
    "@types/redis": "^4.0.11",
    "@types/ws": "^8.5.7",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.60.0",
    "afloat": "^1.0.16",
    "alemonjs": "^1.1.24",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.0.3"
  },
  "workspaces": [
    "plugins/*"
  ],
  "engines": {
    "node": ">=16.14.0"
  }
}
